{
	"name": "mole",
	"version": "1.0.0-alpha.6",
	"description": "A tool for managing design decision outputs for different platforms",
	"main": "dist/index.js",
	"scripts": {
		"build": "babel src -d dist --copy-files && NODE_ENV=test node dist/index.js --inspect",
		"prod": "babel src -d dist --copy-files && NODE_ENV=production node dist/index.js",
		"jsonnet": "jsonnet src/default-config.jsonnet -o src/default-config.json",
		"test": "jest --verbose",
		"dev": "jest --watch",
		"docs": "node_modules/.bin/jsdoc --configure jsdoc.config.js --verbose"
	},
	"author": "Gavin McFarland",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/limitlessloop/mole.git"
	},
	"license": "CC0-1.0",
	"devDependencies": {
		"@babel/cli": "^7.10.3",
		"@babel/core": "^7.10.3",
		"@babel/node": "^7.10.3",
		"@babel/plugin-proposal-class-properties": "^7.10.1",
		"@babel/preset-env": "^7.10.3",
		"@rbicker/jsonnet": "^0.1.1-0.12.1",
		"babel-plugin-add-module-exports": "^1.0.2",
		"babel-plugin-rewire": "^1.2.0",
		"babel-preset-minify": "^0.5.1",
		"jest": "^26.1.0",
		"jsonnet-exec": "^0.1.0",
		"minami": "^1.2.3",
		"ms": "^2.1.2",
		"node-addon-jsonnet": "^1.0.17",
		"prettier": "^2.0.5"
	},
	"dependencies": {
		"@unboundedsystems/jsonnet": "^0.9.4-rc6",
		"fs-extra": "^9.0.1",
		"glob": "^7.1.6",
		"immutable": "^4.0.0-rc.12",
		"lodash.clonedeep": "^4.5.0",
		"lodash.merge": "^4.6.2",
		"nunjucks": "^3.2.1"
	},
	"jest": {
		"testEnvironment": "node",
		"roots": [
			"src",
			"__tests__"
		]
	},
	"keywords": [
		"design",
		"design systems",
		"design tokens",
		"css",
		"ios",
		"android"
	]
}
